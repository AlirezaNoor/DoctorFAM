@model DoctorFAM.Domain.ViewModels.DoctorPanel.Appointment.AddReservationDateViewModel

<form id="AddReservationDate" asp-action="AddPersonalWorkInSampleInModal" asp-controller="SellerPersonalInfo" asp-area="Seller" data-ajax="true" data-ajax-success="AddReservationDateSubmited">
    <div class="modal-body">
        <div class="row">

            <div class="form-group col-md-12 col-sm-12">
                <label asp-for="ReservationDate"></label>
                            <input type="text" class="form-control" id="datepicker" AdminDatePicker="1" style="z-index:9999!important;" asp-for="ReservationDate">
                <span class="text-danger" asp-validation-for="ReservationDate"></span>
            </div>

            <div class="form-group col-md-12">
                <button type="submit" class="btn btn-success">@SharedLocalizer["Submit"]</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">@SharedLocalizer["Close"]</button>
            </div>

        </div>
    </div>

</form>