@model List<DoctorFAM.Domain.Entities.HealthInformation.HealthInformation>
@using DoctorFAM.Application.StaticTools



@{
    ViewData["Title"] = "تیوی فم";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts
    {

    <script>

        $(document).on('click', '#videoPreview', function (e) {

            const videoSource = $(this).attr('data-src');

            $('#TVFAM source').attr('src', videoSource);
            $('#TVFAM')[0].load();
            $('#TVFAM')[0].play();

        });


    </script>

}
<div class="page-content">

    <section>
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 order-lg-12">
                    <div class="row  align-items-center">
                        <div class="col-md-5">
                            <h5 class="text-capitalize mb-0">  </h5>
                        </div>
                        <div class="col-md-7 sm-mt-3 d-sm-flex justify-content-md-end align-items-center">
                            <div class="sort-filter   align-items-center">
                                <select>
                                    <option> همه موضوعات </option>
                                    <option> جدیدترین  </option>
                                    <option>محبوبترین</option>
                                    <option> پزشک خانواده </option>
                                    <option> دیابت </option>
                                    <option> فشار خون </option>
                                    <option> تغذیه </option>
                                    <option> سالمندان </option>
                                    <option> سرطان </option>
                                    <option> پوست و زیبایی </option>
                                    <option> آزمایشها و پاراکلینیک </option>
                                    <option> مادر و کودک</option>
                                    <option> طب سنتی </option>
                                    <option> روان </option>

                                </select>
                            </div>
                            
                        </div>
                    </div>
                    @await Component.InvokeAsync("TVFAMLandingPage")

                </div>
               
            </div>
        </div>
    </section>

</div>